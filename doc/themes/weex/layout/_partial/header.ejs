              <li>
                <a href="<%= pick_lang('en') %>" data-lang="en">English</a>
              </li>
              <li>
                <a href="<%= pick_lang('cn') %>" data-lang="zh-cn">中文</a>
              </li>
            </ul>
          </div>
        </li>
